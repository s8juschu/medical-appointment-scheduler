server {
        listen 80;
        listen [::]:80;

        access_log /var/log/nginx/badbe-access.log;
        error_log /var/log/nginx/badbe-error.log;

        location /admin/ {
                    proxy_pass http://127.0.0.1:8000;
        }

        location /static/admin/ {
                    proxy_pass http://127.0.0.1:8000;
        }

        location / {
                    proxy_pass http://127.0.0.1:4200;
        }
}
